{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules":{
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
        "indent": [true, "spaces", 2],
        "only-arrow-functions": false,
        "no-console": false,
        "quotemark": [true, "single", "jsx-double"]
    },
    "rulesDirectory": []
}